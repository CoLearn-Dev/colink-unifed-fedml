{
  "framework": "fedml",
  "dataset": "breast_horizontal",
  "algorithm": "fed_avg",
  "model": "logistic_regression",
  "deployment": {
    "mode": "colink",
    "participants": [
      [
        "0",
        "server"
      ],
      [
        "1",
        "client"
      ],
      [
        "2",
        "client"
      ],
      [
        "3",
        "client"
      ],
      [
        "4",
        "client"
      ]
    ]
  },
  "training": {
    "epochs": 40,
    "batch_size": 32,
    "inner_step": 5,
    "client_per_round": 2,
    "tot_client_num": 2,
    "learning_rate": 0.01,
    "loss_func": "cross_entropy",
    "optimizer": "adam",
    "optimizer_param": {
      "weight_decay": 0
    }
  }
}