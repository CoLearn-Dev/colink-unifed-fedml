{
  "framework": "fedml",
  "algorithm": "fedavg",
  "dataset": "default_credit_horizontal",
  "model": "mlp_128",
  "deployment": {
    "mode": "colink",
    "participants": [
      [
        "0",
        "server"
      ],
      [
        "1",
        "client"
      ],
      [
        "2",
        "client"
      ]
    ]
  },
  "training": {
    "epochs": 30,
    "inner_step": 1,
    "batch_size": 32,
    "learning_rate": 0.01,
    "client_per_round": 2,
    "tot_client_num": 2,
    "optimizer": "sgd",
    "optimizer_param": {
      "momentum": 0.9,
      "dampening": 0,
      "weight_decay": 0,
      "nesterov": false
    }
  }
}