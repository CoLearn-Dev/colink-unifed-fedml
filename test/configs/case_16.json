{
  "framework": "fedml",
  "dataset": "femnist",
  "algorithm": "fed_avg",
  "model": "mlp_128",
  "mode": "simulation",
  "deployment": {
    "mode": "colink",
    "participants": [
      {
        "user_id": "0",
        "role": "server"
      }
    ]
  },
  "training": {
    "epochs": 2000,
    "batch_size": 8,
    "inner_step": 1,
    "client_per_round": 20,
    "tot_client_num": 178,
    "learning_rate": 0.001,
    "loss_func": "cross_entropy",
    "optimizer": "sgd",
    "optimizer_param": {
      "momentum": 0.9,
      "dampening": 0,
      "weight_decay": 0,
      "nesterov": false
    }
  }
}